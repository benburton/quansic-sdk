{
  "name": "quansic-sdk",
  "description": "A TypeScript REST connector and GraphQL service implementation of the Quansic API.",
  "license": "MIT",
  "module": "index.ts",
  "type": "module",
  "devDependencies": {
    "bun-types": "latest",
    "prettier": "^3.1.0"
  },
  "scripts": {
    "codegen": "bun run src/scripts/codegen.ts",
    "dev": "bun run --watch src/server.ts",
    "fmt": "prettier --write --cache './{src,test,bench,packages/{bun-types,bun-inspector-*,bun-vscode,bun-debug-adapter-protocol}}/**/*.{mjs,ts,tsx,js,jsx}'"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@graphql-codegen/core": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@graphql-tools/graphql-file-loader": "^8.0.0",
    "@graphql-tools/load": "^8.0.0",
    "axios": "^1.6.2",
    "dataloader": "^2.2.2",
    "graphql": "^16.8.1",
    "graphql-yoga": "^5.0.0",
    "zod": "^3.22.4"
  }
}